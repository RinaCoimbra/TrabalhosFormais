SKIP :
{ " " | "\r" | "\t" | "\n" }
TOKEN :
{   
	<P:"Program"> 
	| <ID:["a"-"z", "A"-"Z"] (["a"-"z", "A"-"Z", "0"-"9"])*>
	| <INT:(["0"-"9"])+>
}

TOKEN :
{
    < BEGIN: "BEGIN" >
  | < ESCOPO:["{"] (~["}"])* ["}"]>
}

TOKEN :
{
  <COMMENT: ["#"](~["\n"])+>
}
TOKEN :
{   
	< END:";" >
}
int Parse():		{}
{   
	<P><ID><END>  {return 1;}
}

int Comment(): {}
{
  <COMMENT> {return 1;}
}

int Parse2():		{int v,t;}
{   
	t=Primary(){v=t;} "+" t=Primary(){v+=t;}{return v;}
}

int Primary():		{Token t;}
{   
	t = <INT>  {return Integer.parseInt(t.image);}
}